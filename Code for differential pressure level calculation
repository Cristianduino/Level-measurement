            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(L1.Text)) - CDbl(Val(LT.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 + factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext

        ElseIf CAJATEXTO.Text = "elevación de cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 - factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext


        ElseIf EULRV.SelectedItem = "Psi" Then
            factor2 = 0.03613
            Label21.Text = "Psi"
            Label22.Text = "Psi"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor2)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor2)) / factor5
            URV.Text = urvtext


        ElseIf EULRV.SelectedItem = "mBar" Then
            factor3 = 2.49082
            Label21.Text = "mBar"
            Label22.Text = "mBar"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor3)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor3)) / factor5
            URV.Text = urvtext


        ElseIf EULRV.SelectedItem = "mmH2O" Then
            factor4 = 25.4
            Label21.Text = "mmH2O"
            Label22.Text = "mmH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor4)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor4)) / factor5
            URV.Text = urvtext
        End If
    End Sub

    Private Sub LT_TextChanged_1(sender As Object, e As EventArgs) Handles LT.TextChanged
        DIF()
    End Sub

    Private Sub texto()

        If H1.Text > 0 Then
            CAJATEXTO.Text = "elevación de cero"
        ElseIf H1.Text = 0 Then
            CAJATEXTO.Text = "cero"
        ElseIf H1.Text < 0 Then
            CAJATEXTO.Text = "supresión de cero"
        End If

    End Sub

    Private Sub H1_TextChanged(sender As Object, e As EventArgs) Handles H1.TextChanged
        texto()

        If CAJATEXTO.Text = "cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor1)) / factor5
            URV.Text = urvtext


        ElseIf CAJATEXTO.Text = "supresión de cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(L1.Text)) - CDbl(Val(LT.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 + factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext

        ElseIf CAJATEXTO.Text = "elevación de cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 - factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext
