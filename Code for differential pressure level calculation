    'SGLOW.text sera igual a SGHIGH.text.

    Private Sub SGHIGH_TextChanged(sender As Object, e As EventArgs) Handles SGHIGH.TextChanged
        If Main.Identico.Checked = True Then
            SGLOW.Text = SGHIGH.Text

        End If

    End Sub

