    Public Sub SGLO_SelectedIndexChanged(sender As Object, e As EventArgs) Handles SGLO.SelectedIndexChanged

        If SGLO.SelectedItem = "FC 77" Then
            SGLOW.Text = "1.77"
        ElseIf SGLO.SelectedItem = "Fluoruaceite MO 10" Then
            SGLOW.Text = "1.91"
        ElseIf SGLO.SelectedItem = "Fluoro oil MO 10" Then
            SGLOW.Text = "1.91"
        ElseIf SGLO.SelectedItem = "Fomblim" Then
            SGLOW.Text = "1.87"
        ElseIf SGLO.SelectedItem = "Glicerina 50 % y Agua 50 %" Then
            SGLOW.Text = "1.13"
        ElseIf SGLO.SelectedItem = "50% Glycerine 50 % water" Then
            SGLOW.Text = "1.13"
        ElseIf SGLO.SelectedItem = "Halocarbono" Then
            SGLOW.Text = "1.85"
        ElseIf SGLO.SelectedItem = "Halocarbon" Then
            SGLOW.Text = "1.85"
        ElseIf SGLO.SelectedItem = "Krytox" Then
            SGLOW.Text = "1.88"
        ElseIf SGLO.SelectedItem = "Propilen glycol y agua" Then
            SGLOW.Text = "1.02"
        ElseIf SGLO.SelectedItem = "Propilen glycol and water" Then
            SGLOW.Text = "1.02"
        ElseIf SGLO.SelectedItem = "Neobee M 20" Then
            SGLOW.Text = "0.92"
        ElseIf SGLO.SelectedItem = "Silicona 200 10 Cst" Then
            SGLOW.Text = "0.934"
        ElseIf SGLO.SelectedItem = "Silicone 200 10 Cst" Then
            SGLOW.Text = "0.934"
        ElseIf SGLO.SelectedItem = "Silicona 200 aplicaciones Vacio" Then
            SGLOW.Text = "0.934"
        ElseIf SGLO.SelectedItem = "Silicone 200 vacuum aplication" Then
            SGLOW.Text = "0.934"
        ElseIf SGLO.SelectedItem = "Silicona 704" Then
            SGLOW.Text = "1.07"
        ElseIf SGLO.SelectedItem = "Silicone 704" Then
            SGLOW.Text = "1.07"
        ElseIf SGLO.SelectedItem = "Silicona 704 aplicaciones Vacio" Then
            SGLOW.Text = "1.07"
        ElseIf SGLO.SelectedItem = "Silicone 704 vacuum aplication" Then
            SGLOW.Text = "1.07"
        ElseIf SGLO.SelectedItem = "Silicona 705" Then
            SGLOW.Text = "1.09"
        ElseIf SGLO.SelectedItem = "Silicone 705" Then
            SGLOW.Text = "1.09"
        ElseIf SGLO.SelectedItem = "Silicona 705 aplicaciones Vacio" Then
            SGLOW.Text = "1.09"
        ElseIf SGLO.SelectedItem = "Silicone 705 vacuum aplication" Then
            SGLOW.Text = "1.09"
        ElseIf SGLO.SelectedItem = "Syltherm 800" Then
            SGLOW.Text = "0.934"
        ElseIf SGLO.SelectedItem = "Syltherm XLT" Then
            SGLOW.Text = "0.85"
        ElseIf SGLO.SelectedItem = "Ultratherm 805" Then
            SGLOW.Text = "1.2"
        ElseIf SGLO.SelectedItem = "Ultratherm 805 aplicaciones Vacio" Then
            SGLOW.Text = "1.2"
        ElseIf SGLO.SelectedItem = "Ultratherm 805 vacuum aplication" Then
            SGLOW.Text = "1.2"
        ElseIf SGLO.SelectedItem = "Definida por el usuario" Then
