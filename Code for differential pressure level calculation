Public Class closedtk

    Public factor1 As Double 'Factor de conversión InH2O
    Public factor2 As Double 'Factor de conversión Psi
    Public factor3 As Double 'Factor de conversión mBar
    Public factor4 As Double 'Factor de conversión mBar
    Public Calc1 As Double ' Calculo para determinar LRV
    Public Calc2 As Double ' Calculo para determinar URV
    Public factor5 As Double 'Factor de conversión In
    Public factor6 As Double
    Public factor7 As Double
    Public factor8 As Double
    Public factor9 As Double
    Public factor10 As Double
    Public factor11 As Double
    Public H1LOW As Double
    Public H1HIGH As Double
    Public H1HIGH1 As Double
    Public lrvtext As Double
    Public urvtext As Double
 
    Public Sub DIF()
        H1.Text = CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))
    End Sub

    Private Property MsgBox As String

    Private Sub Form2_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        EU1.Items.Add("In")
        EU1.Items.Add("mm")
        EU1.Items.Add("Cm")
        EU1.Items.Add("Mts")
        EU1.Items.Add("Ft")

        'Variables dentro de  combobox de unidades de ingenieria presión LRV

        EULRV.Items.Add("InH2O")
        EULRV.Items.Add("Psi")
        EULRV.Items.Add("mBar")
        EULRV.Items.Add("mmH2O")

    End Sub

    'Instrucción usada para ocultar el menú Tk cerrado y visualizar el menú principal.

    Private Sub Atras_Click(sender As Object, e As EventArgs) Handles Atras.Click
        Me.Hide()
        Main.Show()

    End Sub

    Public sub Unitlenght()
        If EU1.SelectedItem = "In" Then
            
        End If


    End Sub


    'Las siguientes instrucciones son usadas para que cuando en el combobox de unidad de ing, 
    'seleccionemos alguna unidad de longitud automaticamente en los label se refresque automaticamente el valor.


    Public Sub EU1_SelectedIndexChanged(sender As Object, e As EventArgs) Handles EU1.SelectedIndexChanged

        If CAJATEXTO.Text = "cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor1)) / factor5
            URV.Text = urvtext

        ElseIf CAJATEXTO.Text = "supresión de cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(L1.Text)) - CDbl(Val(LT.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 + factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext


        ElseIf CAJATEXTO.Text = "elevación de cero" And EU1.SelectedItem = "In" And EULRV.SelectedItem = "InH2O" Then
            Label2.Text = "In"
            Label7.Text = "In"
            Label8.Text = "In"
            Label10.Text = "In"
            Label12.Text = "In"
            Label13.Text = "In"
            Label15.Text = "In"
            factor5 = 1
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"

            factor7 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGLOW.Text))
            factor6 = (CDbl(Val(L1.Text)) - CDbl(Val(L2.Text))) * CDbl(Val(SGLOW.Text))
            H1LOW = factor7 + factor6
            factor8 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor9 = (CDbl(Val(LT.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(SGHIGH.Text))
            H1HIGH = factor8 - factor9
            lrvtext = H1HIGH + H1LOW
            LRV.Text = lrvtext

            H1HIGH1 = CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text))
            urvtext = H1HIGH1 + lrvtext
            URV.Text = urvtext


        ElseIf EU1.SelectedItem = "mm" Then
            Label2.Text = "mm"
            Label7.Text = "mm"
            Label8.Text = "mm"
            Label10.Text = "mm"
            Label12.Text = "mm"
            Label13.Text = "mm"
            Label15.Text = "mm"
            factor5 = 25.4
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = Calc2 * factor1 / factor5
            URV.Text = urvtext

        ElseIf EU1.SelectedItem = "Cm" Then
            Label2.Text = "Cm"
            Label7.Text = "Cm"
            Label8.Text = "Cm"
            Label10.Text = "Cm"
            Label12.Text = "Cm"
            Label13.Text = "Cm"
            Label15.Text = "Cm"
            factor5 = 2.54
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor1)) / factor5
            URV.Text = urvtext

        ElseIf EU1.SelectedItem = "Mts" Then
            Label2.Text = "Mts"
            Label7.Text = "Mts"
            Label8.Text = "Mts"
            Label10.Text = "Mts"
            Label12.Text = "Mts"
            Label13.Text = "Mts"
            Label15.Text = "Mts"
            factor5 = 0.0254
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor1)) / factor5
            URV.Text = urvtext

        ElseIf EU1.SelectedItem = "Ft" Then
            Label2.Text = "Ft"
            Label7.Text = "Ft"
            Label8.Text = "Ft"
            Label10.Text = "Ft"
            Label12.Text = "Ft"
            Label13.Text = "Ft"
            Label15.Text = "Ft"
            factor5 = 0.08333333
            factor1 = 1
            Label21.Text = "InH2O"
            Label22.Text = "InH2O"
            factor7 = (CDbl(Val(L4.Text)) - CDbl(Val(L1.Text))) * CDbl(Val(Densfluid.Text))
            factor6 = CDbl(Val(L3.Text)) * CDbl(Val(SGLOW.Text))
            Calc1 = factor7 - factor6
            lrvtext = (CDbl(Calc1) * CDbl(factor1)) / factor5
            LRV.Text = lrvtext
            factor8 = (CDbl(Val(L6.Text)) * CDbl(Val(Densfluid.Text)))
            Calc2 = factor8 + (CDbl(Calc1) * CDbl(factor1))
            urvtext = (CDbl(Calc2) * CDbl(factor1)) / factor5
            URV.Text = urvtext

        End If

    End Sub
